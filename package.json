{
  "name": "iquery-js",
  "version": "3.0.0",
  "description": "A jQuery compatible API based on built-in ECMA / W3C polyfill",
  "main": "iQuery.js",
  "directories": {
    "doc": "doc"
  },
  "engines": {
    "node": "^5.0.0"
  },
  "devDependencies": {
    "amd-bundle": "^0.3.2",
    "opn-cli": "^3.1.0",
    "uglify-js": "^3.0.22"
  },
  "scripts": {
    "bundle": "cd source  &&  amd-bundle ../iQuery.js",
    "compress": "uglifyjs iQuery.js -o build/iQuery.min.js --config-file build/compress.json",
    "build": "npm run bundle  &&  npm run compress",
    "test": "opn ./test/unit.html"
  },
  "repository": {
    "type": "git",
    "url": "https://git.oschina.net/Tech_Query/iQuery.git"
  },
  "keywords": [
    "jquery",
    "w3c-api",
    "dom",
    "ecma",
    "polyfill"
  ],
  "author": "shiy007@qq.com",
  "license": "GPL-2.0"
}
